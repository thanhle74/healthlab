        <settings>
            <submitUrl path="meal/plan/save"/>
        </settings>
        <dataProvider class="Annam\MealPlan\Model\MealPlanDataProvider" name="meal_plan_form_data_source">
            <settings>
                <requestFieldName>id</requestFieldName>
                <primaryFieldName>id</primaryFieldName>
            </settings>
        </dataProvider>
    </dataSource>
    <fieldset name="general_information">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
